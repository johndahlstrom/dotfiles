snippet func
  function (${1:param}) {
    ${2:code}
  }${0}

snippet funcvar
  var ${1:var} = function (${2:param}) {
    ${3:code}
  }${0}

snippet if
  if (${1:cond}) {
    ${2:code}
  }${0}

snippet else
	else {
		${1}
	}${0}

snippet for
  options head
  for (var ${1:i}=0; $1 < ${2:#:Things}.length; ++$1) {
    ${0:TARGET}
  }

snippet forin
  options head
  for (var ${1:i} in ${2:#:Things}) {
    ${0:TARGET}
  }

snippet cl
  console.log(${1});${0}
